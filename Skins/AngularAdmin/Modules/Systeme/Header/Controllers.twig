                    $scope.modalObj.{{ c.objecttype }}{{ child.name }} = $scope.obj.id;
                    $scope.modalObj.{{ c.objecttype }}{{ child.name }}label = $scope.obj.label;
                    $scope.modalObj.title = 'Ajouter un nouveau {{ child.objectDescription }}';
