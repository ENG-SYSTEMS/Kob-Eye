            <section class="panel">
                <div class="panel-body">
                    <h6>Informations</h6>
                    <dl class="dl-horizontal user-profile-dl">
                        {% for field in fichefields %}
                            <dt>
                                {% if field.parentDescription is not empty %}{{ field.parentDescription }}{% else %}{{ field.description }}{% endif %}
                            </dt>
                            <dd>
                                {% if field.type == "price" %}
                                    {% if attribute(User,field.name) %}
                                        <span class="label label-warning">{{ attribute(User,field.name) | number_format }} €</span>
                                    {% else %}
                                        <i class="icmn-spam"></i> Non renseigné
                                    {% endif %}
                                {% elseif field.type == "float" %}
                                    {% if attribute(User,field.name) %}
                                        <span class="label label-warning">{{ attribute(User,field.name) | number_format }}</span>
                                    {% else %}
                                        <i class="icmn-spam"></i> Non renseigné
                                    {% endif %}
                                {% elseif field.type == "extlink" %}
                                    {% if attribute(User,field.name) %}
                                        <a class="btn btn-success btn-xs" href="https://zabbix.abtel.fr/guacamole/#/client/{{ attribute(User,field.name) }}" target="_blank">{{ field.listDescr }}</a>
                                    {% else %}
                                        <i class="icmn-spam"></i> Non renseigné
                                    {% endif %}
                                {% elseif field.type == "int" %}
                                    {% if attribute(User,field.name) %}
                                        <span class="label label-primary">{{ attribute(User,field.name) }}</span>
                                    {% else %}
                                        <i class="icmn-spam"></i> Non renseigné
                                    {% endif %}
                                {% elseif field.type == "boolean" %}
                                    {% if attribute(User,field.name) %}
                                        <span class="label label-success" ng-if="{{ attribute(User,field.name) }}"><i class="fa fa-check"></i></span>
                                        <span class="label label-danger" ng-if="!{{ attribute(User,field.name) }}"><i class="fa fa-times"></i></span>
                                    {% else %}
                                        <i class="icmn-spam"></i> Non renseigné
                                    {% endif %}
                                {% elseif field.type == "fkey" and field.card == "short" %}
                                    {% if attribute(User,field.name) %}
                                        {% set temp =  field.name  ~ 'label' %}
                                        <a class="btn btn-primary btn-xs" href="#{{ field.link }}/{{ attribute(User,field.name) }}">{{ attribute(User,temp) }}</a>
                                    {% else %}
                                        <i class="icmn-spam"></i> Non renseigné
                                    {% endif %}
                                {% else %}
                                    {% if attribute(User,field.name) %}
                                        <span >{{ attribute(User,field.name) }}</span>
                                    {% else %}
                                        <i class="icmn-spam"></i> Non renseigné
                                    {% endif %}
                                {% endif %}
                            </dd>
                        {% endfor %}
                        <dt>Adresse</dt>
                        {% if adresse %}
                            <dd>{{ adresse }}</dd>
                        {% else %}
                            <i class="icmn-spam"></i> Non renseigné
                        {% endif %}
                    </dl>
                    {% if adresse %}
                        <iframe src="https://www.google.com/maps?q={{ adresse|url_encode }}&output=embed"></iframe>
                    {% endif %}


                    <div class="btn-group-vertical btn-group-justified">
                        <button type="button" class="btn">Modifier mes informations</button>
                    </div>
                </div>
            </section>
