{# Attestations Adherent #}
{% from "Skins/AngularAdmin/Modules/Cadref/Default/form_components.twig" import label %} 
<div class="page-content-inner">
    <h3>Fiches de pr√©sence</h3>
    <!-- Default Panel -->
    <section class="panel">
        <div class="panel-body cadref-panel-body">
            <div class="row no-gutters">
                <div class="col-md-12">
					<div class="row">
						<div class="col-md-1">
							<label class="form-control-label cadref-label" style="line-height:1">Classe</label>
						</div>
						<div class="col-md-4">
							<select class="form-control cadref-input" ng-model="Presence.Debut">
								{% for k,v in classes %}
									<option value="{{ k }}">{{ v }}</option>
								{% endfor %}
							</select>
						</div>
					</div>
					<div class="row" style="height:10px"></div>
					<div class="row">
						<div class="col-md-1">
							{{ label("Mois") }}
						</div>
						<div class="col-md-2">
							<select class="form-control cadref-input" ng-model="Presence.Mois" ng-init="Presence.Mois='{{ encours }}'">
								{% for k,v in mois %}
									<option value="{{ k }}">{{ v }}</option>
								{% endfor %}
							</select>
						</div>
					</div>
					<div class="row" style="height:10px"></div>
					<div class="row">
						<div class="col-md-1">
						</div>
						<div class="col-md-6">
							<button type="button" class="btn btn-success margin-inline " ng-click="enseignantPresence()">Imprimer</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
</div>


