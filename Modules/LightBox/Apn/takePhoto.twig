{% if obj.Error|length %}
    <div class="alert alert-danger">
        <ul>
            {% for er in obj.Error %}
                <li>{{ er.Message }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
{% if obj.Warning|length %}
    <div class="alert alert-warning">
        <ul>
            {% for er in obj.Warning %}
                <li>{{ er.Message }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
{% if obj.Success|length %}
    <div class="alert alert-success">
        <ul>
            {% for er in obj.Success %}
                <li>{{ er.Message }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
{% if success %}
<div class="row">
    <div class="cold-md-12">
        <img class="img-responsive" src="/{{ success }}" style="width: 100%;"/>
    </div>
</div>
{% endif %}

