{% if obj.Error|length %}
    <div class="alert alert-danger">
        <ul>
            {% for er in obj.Error %}
                <li>{{ er.Message }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
{% if obj.Warning|length %}
    <div class="alert alert-warning">
        <ul>
            {% for er in obj.Warning %}
                <li>{{ er.Message }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
{% if obj.Success|length %}
    <div class="alert alert-success">
        <ul>
            {% for er in obj.Success %}
                <li>{{ er.Message }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
{% if success %}
    <div class="alert alert-success">Une tache de point de création d'un point de restauration a été créé et sera éxécuté sous peu. Rendez vous dasn l'onglet "Taches" pour suivre l'éxécution de la commande.</div>
{% endif %}
